{
  "name": "transit-tracker-api",
  "version": "0.0.1",
  "description": "",
  "author": "TJ Horner <me@tjhorner.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "nest build && pnpm sentry:sourcemaps",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "SENTRY_ENVIRONMENT=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "kysely-codegen": "kysely-codegen --out-file src/modules/feed/modules/gtfs/db.d.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tj-horner-dw --project transit-tracker-api ./dist && sentry-cli sourcemaps upload --org tj-horner-dw --project transit-tracker-api ./dist",
    "gtfs:sync": "nest start --entryFile scripts/sync-gtfs",
    "gtfs:db:migrate": "dbmate -e SUPERUSER_DATABASE_URL up"
  },
  "dependencies": {
    "@anchan828/nest-redlock": "^0.3.10",
    "@keyv/redis": "4.3.3",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.13",
    "@nestjs/core": "^11.0.13",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/platform-ws": "^11.0.13",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.13",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-prometheus": "^0.200.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@sentry/cli": "^2.43.0",
    "@sentry/nestjs": "^9.11.0",
    "@turf/turf": "^7.2.0",
    "axios": "^1.8.4",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "4.1.4",
    "dbmate": "^2.26.0",
    "fast-csv": "^5.0.2",
    "geojson": "^0.5.0",
    "gtfs-realtime-bindings": "^1.1.1",
    "ioredis": "^5.6.0",
    "js-yaml": "^4.1.0",
    "keyv": "5.3.2",
    "kysely": "^0.27.6",
    "limiter": "^3.0.0",
    "nestjs-kysely": "^1.2.0",
    "nestjs-otel": "^6.2.0",
    "node-fetch": "3.3.2",
    "onebusaway-sdk": "^1.8.4",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "temp-dir": "^3.0.0",
    "unzipper": "^0.12.3",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.4",
    "@nestjs/testing": "^11.0.13",
    "@types/express": "^5.0.1",
    "@types/geojson": "^7946.0.16",
    "@testcontainers/postgresql": "^10.23.0",
    "@testcontainers/redis": "^10.23.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.11",
    "@types/redlock": "^4.0.7",
    "@types/supertest": "^6.0.3",
    "@types/unzipper": "^0.10.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "kysely-codegen": "^0.18.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}